{ 
    "definitions": {
        "Function": {
            "oneOf": [
                {
                    "type": "string",
                    "pattern": "^=>"
                },
                {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "pattern": "^(\\?.+|[ \t]*default *)=>.*$"
                    }
                }
            ]
        },
