                {
                    "type": "string",
                    "pattern": "#([0-9A-F]{3}|[0-9A-F]{6})"
                },
                {
                    "type": "string",
                    "pattern": "rgba([0-9]{1-3},[0-9]{1-3},[0-9]{1-3}(,[0-9]{1-3})?)"
